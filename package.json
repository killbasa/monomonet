{
	"name": "monomonet-fansite",
	"version": "0.0.1",
	"author": "killbasa",
	"license": "0BSD",
	"type": "module",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/killbasa/monomonet.git"
	},
	"scripts": {
		"build": "vite build",
		"dev": "vite dev",
		"preview": "vite preview",
		"sync": "svelte-kit sync",
		"check": "yarn sync && svelte-check --tsconfig ./tsconfig.json",
		"lint": "eslint --fix",
		"format": "prettier --write .",
		"prepare": "husky"
	},
	"devDependencies": {
		"@killbasa/eslint-config": "^3.1.5",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.48.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/node": "^24.10.0",
		"eslint": "9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.43.4",
		"svelte-check": "^4.3.3",
		"typescript": "5.9.3",
		"vite": "7.2.1",
		"wrangler": "^4.46.0"
	},
	"lint-staged": {
		"*.{mjs,cjs,js,ts}": [
			"eslint --fix --ext js,ts",
			"prettier --write --log-level=warn \"**/*.{mjs,cjs,js,ts}\""
		],
		"*.{json,yml,yaml,md}": [
			"prettier --write --log-level=warn \"**/*.{json,yml,yaml,md}\""
		]
	},
	"engines": {
		"node": ">=22.x"
	},
	"packageManager": "yarn@4.10.3",
	"volta": {
		"node": "22.21.1"
	}
}
