{
	"name": "monomonet-fansite",
	"version": "0.0.1",
	"author": "killbasa",
	"license": "0BSD",
	"type": "module",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/killbasa/monomonet.git"
	},
	"scripts": {
		"prepare": "husky",
		"dev": "vite",
		"build": "tsc && vite build --emptyOutDir",
		"preview": "vite preview",
		"lint": "eslint . --fix",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@killbasa/eslint-config": "^3.1.5",
		"@killbasa/ts-config": "^1.0.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.48.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/node": "^24.10.0",
		"@typescript-eslint/eslint-plugin": "^8.46.3",
		"@typescript-eslint/parser": "^8.46.3",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-svelte": "^3.13.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"sass": "^1.93.3",
		"svelte": "^5.43.2",
		"svelte-check": "^4.3.3",
		"svelte-eslint-parser": "^1.4.0",
		"svelte-sitemap": "^2.7.1",
		"typescript": "^5.9.3",
		"vite": "^7.1.12",
		"zod": "^4.1.12"
	},
	"lint-staged": {
		"*.{mjs,cjs,js,ts}": [
			"eslint --fix --ext js,ts",
			"prettier --write --log-level=warn \"**/*.{mjs,cjs,js,ts}\""
		],
		"*.{json,yml,yaml,md}": [
			"prettier --write --log-level=warn \"**/*.{json,yml,yaml,md}\""
		]
	},
	"engines": {
		"node": ">=22.x"
	},
	"packageManager": "yarn@4.10.3",
	"volta": {
		"node": "22.21.1"
	}
}
